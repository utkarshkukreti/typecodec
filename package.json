{
  "name": "typecodec",
  "version": "0.15.0",
  "description": "Fast, type-safe runtime validation of unknown data for TypeScript.",
  "keywords": [
    "typescript",
    "schema",
    "validation",
    "unknown"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/utkarshkukreti/typecodec"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "packageManager": "pnpm@10.5.2",
  "scripts": {
    "build": "tsc && tsup src/index.ts --format cjs,esm --dts --clean",
    "test": "tsc && vitest",
    "prepare": "pnpm build",
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "bench": "vite-node benchmarks/index.ts"
  },
  "devDependencies": {
    "@types/node": "22.13.7",
    "prettier": "3.5.2",
    "tinybench": "3.1.1",
    "tsup": "8.4.0",
    "typescript": "5.8.2",
    "vite-node": "3.0.7",
    "vitest": "3.0.7"
  }
}
